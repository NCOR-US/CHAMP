        rdfs:subClassOf :CalculatingTool ;
        obo:IAO_0000117 "Ruoyu Yang" ;
        cco:definition "A calculating artifact that is designed for mathematical calculation consisting of a frame set with rods on which balls or beads are moved ."@en ;
