{
    "type": "artifact-specs",
    "id": "artifactID",
    "iriBase": "gen:Spec-Artifact",
    "baseClass": "cco:Artifact",
    "attributes": [
        {
            "name": "name",
            "structure": [
                "?artifactID cco:designated_by ?ice0",
                "?ice0 rdf:type cco:DesignativeName",
                "?ice0 cco:inheres_in ?ibe0",
                "?ibe0 rdf:type cco:InformationBearingEntity",
                "?ibe0 cco:has_text_value ?name"
            ],
            "generate": [
                {
                    "variable": "ibe0",
                    "strategy": "random",
                    "base": "DesignativeNameIBE"
                },
                {
                    "variable": "ice0",
                    "strategy": "random",
                    "base": "DesignativeNameICE"
                }
            ]
        },
        {
            "name": "amount",
            "structure": [
                "?artifactID lro:has_amount ?amount"
            ]
        },
        {
            "name": "unitOfMeasure",
            "structure": [
                "?artifactID lro:uses_measurement_unit ?unitOfMeasure"
            ]
        }
    ],
    "relationships": [
        {
            "name": "inputOf",
            "type": "sub-process-specs",
            "linkType": "hasMany",
            "structure": [
                "?artifactID lro:is_input_of ?inputOf"
            ]
        },
        {
            "name": "outputOf",
            "type": "sub-process-specs",
            "linkType": "hasMany",
            "structure": [
                "?artifactID lro:is_output_of ?outputOf"
            ]
        }
    ]
}
