tool:CreepTestingTool rdf:type owl:Class ;
                       rdfs:subClassOf tool:TestingTool ;
                       obo:IAO_0000115 "A tool thatis designed to measure some entity's continuing dimensional change while under timed stress load."@en ;
                       obo:IAO_0000117 "BinBin Zhang" ;
                       rdfs:label "Creep Testing Tool" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Crucible
tool:Crucible rdf:type owl:Class ;
               rdfs:subClassOf cco:Container ;
               obo:IAO_0000117 "Ruoyu Yang" ;
               cco:definition "A container that is designed for heating metal, glass and pigment production and can withstand very high temperatures." ;
               rdfs:label "Crucible" .


###  http://www.semanticweb.org/ontologies/ToolOntology#CuttingTool
tool:CuttingTool rdf:type owl:Class ;
                  rdfs:subClassOf cco:Tool ;
                  obo:IAO_0000115 "An artifact that is designed for the separation of a physical object, into two or more portions or create a incision, through the application of an acutely directed force."@en ;
                  obo:IAO_0000117 "Ruoyu Yang" ;
                  obo:IAO_0000119 "https://en.wikipedia.org/wiki/Cutting" ;
                  rdfs:label "Cutting Artifact" .


###  http://www.semanticweb.org/ontologies/ToolOntology#DepthGauge
