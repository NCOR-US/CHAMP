@prefix : <http://www.semanticweb.org/ontologies/ToolOntology> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix cco: <http://www.ontologyrepository.com/CommonCoreOntologies/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix tool: <http://www.semanticweb.org/ontologies/ToolOntology#> .
@prefix ace_lexicon: <http://attempto.ifi.uzh.ch/ace_lexicon#> .
@prefix CEO-ontology: <http://www.semanticweb.org/no/ontologies/2017/1/CEO-ontology#> .
@prefix PLC-ontology: <http://www.semanticweb.org/no/ontologies/2017/1/PLC-ontology#> .
@base <http://www.semanticweb.org/ontologies/ToolOntology> .

<http://www.semanticweb.org/ontologies/ToolOntology> rdf:type owl:Ontology ;
                                                      owl:imports <http://www.semanticweb.org/no/ontologies/2017/1/PLC-ontology> ;
                                                      owl:versionInfo "Version .01"@en ,
                                                                      "J. Neil Otte, Ron Rudnicki, Ruoyu Yang, Binbin Zhang, and Munira Binti Mohd Ali"@en ;
                                                      obo:IAO_0000232 "The Tool Ontology is a domain ontology intended to describe artifacts, loosely defined as a artifact, especially one held in the hand, used to carry out a particular function."@en .

#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/ontologies/ToolOntology#Abacus
tool:Abacus rdf:type owl:Class ;
             rdfs:subClassOf tool:CalculatingTool ;
             obo:IAO_0000117 "Ruoyu Yang" ;
             cco:definition "A calculating artifact that is designed for mathematical calculation consisting of a frame set with rods on which balls or beads are moved ."@en ;
             rdfs:label "Abacus" .


###  http://www.semanticweb.org/ontologies/ToolOntology#AbradingTool
tool:AbradingTool rdf:type owl:Class ;
                   rdfs:subClassOf cco:Tool ;
                   obo:IAO_0000117 "Ruoyu Yang" ;
                   obo:IAO_0000119 "https://en.wikipedia.org/wiki/Abrasion_(mechanical)" ;
                   cco:definition "An artifact that is designed for scuffing, scratching, wearing down, marring, or rubbing away  surface of artifact."@en ;
                   rdfs:label "Abrading Artifact" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Acoustic_Emission_Testing_Tool
tool:Acoustic_Emission_Testing_Tool rdf:type owl:Class ;
                                     rdfs:subClassOf tool:TestingTool ;
                                     obo:IAO_0000115 "A artifact that is designed to test the acoustic emmisions produced by some entity."@en ;
                                     obo:IAO_0000117 "BinBin Zhang" ;
                                     rdfs:label "Acoustic Emission Testing Tool" .


###  http://www.semanticweb.org/ontologies/ToolOntology#AirClipper
tool:AirClipper rdf:type owl:Class ;
                 rdfs:subClassOf tool:CuttingTool ;
                 obo:IAO_0000117 "Ruoyu Yang" ;
                 cco:definition "A cutting artifact that is designed for cutting or trimming small pieces off a material entity."@en ;
                 rdfs:label "Air Clipper" .


###  http://www.semanticweb.org/ontologies/ToolOntology#AirDrill
tool:AirDrill rdf:type owl:Class ;
               rdfs:subClassOf tool:Drill ;
               obo:IAO_0000117 "Ruoyu Yang" ;
               cco:definition "A drill that is powered by compressed air."@en ;
               rdfs:label "Air Drill" .


###  http://www.semanticweb.org/ontologies/ToolOntology#AirFilter
tool:AirFilter rdf:type owl:Class ;
                rdfs:subClassOf tool:ElectricCleaningTool ;
                obo:IAO_0000117 "Ruoyu Yang" ;
                obo:IAO_0000119 "https://en.wikipedia.org/wiki/Air_filter" ;
                cco:definition "An electric cleaning artifact that is designed for removing solid particulates such as dust, pollen, mould, and bacteria from the air."@en ;
                cco:elucidation "Filters contains an absorbent such as charcoal that can remove odors and gaseous pollutants such as volatile organic compounds or ozone." ;
                rdfs:label "Air Filter" .


###  http://www.semanticweb.org/ontologies/ToolOntology#AirGrinder
tool:AirGrinder rdf:type owl:Class ;
                 rdfs:subClassOf tool:AbradingTool ;
                 obo:IAO_0000117 "Ruoyu Yang" ;
                 obo:IAO_0000119 "https://en.wikipedia.org/wiki/Grinding_machine" ;
                 cco:definition "An abrading artifact that is designed for grinding, which is a type of machining using an abrasive wheel as the cutting artifact, powered by compressed air."@en ;
                 rdfs:label "Air Grinder" .


###  http://www.semanticweb.org/ontologies/ToolOntology#AirSander
tool:AirSander rdf:type owl:Class ;
                rdfs:subClassOf tool:AbradingTool ;
                obo:IAO_0000117 "Ruoyu Yang" ;
                obo:IAO_0000119 "https://en.wikipedia.org/wiki/Sander" ;
                cco:definition "An abrading artifact powered by air pressure that is designed for smoothing an object's surface with sandpaper or other abrasive material."@en ;
                rdfs:label "Air Sander" .


###  http://www.semanticweb.org/ontologies/ToolOntology#AirWrench
tool:AirWrench rdf:type owl:Class ;
                rdfs:subClassOf tool:Wrench ;
                obo:IAO_0000117 "Ruoyu Yang" ;
                cco:definition "A wrench that is powered by compressed air."@en ;
                rdfs:label "Air Wrench" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Alarm
tool:Alarm rdf:type owl:Class ;
            rdfs:subClassOf tool:ProtectionTool ;
            obo:IAO_0000117 "Ruoyu Yang" ;
            obo:IAO_0000119 "https://en.wikipedia.org/wiki/Alarm_device" ;
            cco:definition "A protection artifact that gives an audible, visual or other form of alarm signal about a problem or condition."@en ;
            rdfs:label "Alarm" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Anchor
tool:Anchor rdf:type owl:Class ;
             rdfs:subClassOf tool:JoinTool ;
             obo:IAO_0000115 "A join artifact that is designed for connecting a vessel to the bed of a body of water to prevent the craft from drifting due to wind or current."@en ;
             obo:IAO_0000117 "Ruoyu Yang" ;
             obo:IAO_0000119 "https://en.wikipedia.org/wiki/Anchor" ;
             rdfs:label "Anchor" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Artifact_Element
tool:Artifact_Element rdf:type owl:Class ;
                       rdfs:subClassOf cco:Artifact ;
                       cco:definition "An artifact that is designed to be an elementary component of some artifact." ;
                       cco:term_editor "Ruoyu Yang" ;
                       rdfs:label "Artifact Element" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Axe
tool:Axe rdf:type owl:Class ;
          rdfs:subClassOf tool:CuttingTool ;
          obo:IAO_0000117 "Ruoyu Yang" ;
          cco:definition "A cutting artifact that is designed for chopping wood with a steel blade attached at a right angle to a handle."@en ;
          rdfs:label "Axe" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Bag
tool:Bag rdf:type owl:Class ;
          rdfs:subClassOf cco:Container ;
          obo:IAO_0000117 "Ruoyu Yang" ;
          cco:definition "A container that is designed for carrying things ,made of flexible material with an opening at the top." ;
          rdfs:label "Bag" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Ball-PointPen
tool:Ball-PointPen rdf:type owl:Class ;
                    rdfs:subClassOf tool:WritingTool ;
                    obo:IAO_0000117 "Ruoyu Yang" ;
                    cco:definition "A writing artifact that dispenses ink over a metal ball at its point."@en ;
                    rdfs:label "Ball-Point Pen" .


###  http://www.semanticweb.org/ontologies/ToolOntology#BandSaw
tool:BandSaw rdf:type owl:Class ;
              rdfs:subClassOf tool:SawingTool ;
              obo:IAO_0000115 "A sawing artifact that is designed for woodworking, metalworking, and lumbering with a long, sharp blade consisting of a continuous band of toothed metal stretched between two or more wheels to cut material."@en ;
              obo:IAO_0000117 "Ruoyu Yang" ;
              obo:IAO_0000119 "https://en.wikipedia.org/wiki/Bandsaw" ;
              rdfs:label "Band Saw" .


###  http://www.semanticweb.org/ontologies/ToolOntology#BarCode
tool:BarCode rdf:type owl:Class ;
              rdfs:subClassOf tool:SignArtifact ;
              obo:IAO_0000117 "Ruoyu Yang" ;
              cco:definition "A sign artifact that conatins optical, machine-readable data, which usually describes information about the object." ;
              rdfs:label "BarCode" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Basket
tool:Basket rdf:type owl:Class ;
             rdfs:subClassOf cco:Container ;
             obo:IAO_0000115 "A container that is designed for holding or carrying objects, typically made from interwoven strips of cane or wire." ;
             obo:IAO_0000117 "Ruoyu Yang" ;
             rdfs:label "Basket" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Beaker
tool:Beaker rdf:type owl:Class ;
             rdfs:subClassOf cco:Container ;
             obo:IAO_0000117 "Ruoyu Yang" ;
             cco:definition "A container that is containers for stirrung, mixing and heating liquids and generally cylindrical in shape with a flat bottom." ;
             rdfs:label "Beaker" .


###  http://www.semanticweb.org/ontologies/ToolOntology#BladeGrinder
tool:BladeGrinder rdf:type owl:Class ;
                   rdfs:subClassOf tool:ElectricGrinder ;
                   obo:IAO_0000115 "An electric grinder that is designed for chopping material while mixing it, by means of a high-speed spinning blade."@en ;
                   obo:IAO_0000117 "Ruoyu Yang" ;
                   obo:IAO_0000119 "https://en.wikipedia.org/wiki/Blade_grinder" ;
                   rdfs:label "Blade Grinder" .


###  http://www.semanticweb.org/ontologies/ToolOntology#BleederResistanceTester
tool:BleederResistanceTester rdf:type owl:Class ;
                              rdfs:subClassOf tool:TestingTool ;
                              cco:definition "A testing artifact that is designed for testing the bleeder resistance inhering in a material entity."@en ;
                              cco:term_editor "J. Neil Otte"@en ;
                              rdfs:label "Bleeder Resistance Tester" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Bolt
tool:Bolt rdf:type owl:Class ;
           rdfs:subClassOf tool:Artifact_Element ;
           cco:definition "An artifact element that is designed as threaded fastener with an external male thread." ;
           cco:definition_source "https://en.wikipedia.org/wiki/Bolt_(fastener)" ;
           cco:term_editor "Ruoyu Yang" ;
           rdfs:label "Bolt" .


###  http://www.semanticweb.org/ontologies/ToolOntology#BoltCutter
tool:BoltCutter rdf:type owl:Class ;
                 rdfs:subClassOf tool:CuttingTool ;
                 obo:IAO_0000115 "A cutting artifact that is designed for cutting chains, padlocks, bolts, and wire mesh."@en ;
                 obo:IAO_0000117 "Ruoyu Yang" ;
                 obo:IAO_0000119 "https://en.wikipedia.org/wiki/Bolt_cutter" ;
                 cco:elucidation "Bolt cutter has long handles and short blades, with compound hinges to maximize leverage and cutting force." ;
                 rdfs:label "Bolt Cutter" .


###  http://www.semanticweb.org/ontologies/ToolOntology#BoreGauge
tool:BoreGauge rdf:type owl:Class ;
                rdfs:subClassOf tool:DimensionalMeasurementTool ;
                obo:IAO_0000117 "Ruoyu Yang" ;
                obo:IAO_0000119 "https://en.wikipedia.org/wiki/Bore_gauge" ;
                cco:definition "A spatial measurement artifact that is designed for measuring or transfering artifacts that are used in the process of accurately measuring holes."@en ;
                rdfs:label "Bore Gauge" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Bowl
tool:Bowl rdf:type owl:Class ;
           rdfs:subClassOf cco:Container ;
           obo:IAO_0000115 "A container that is designed for serving hot and cold food, which is round and open-top." ;
           obo:IAO_0000117 "Ruoyu Yang" ;
           rdfs:label "Bowl" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Box
tool:Box rdf:type owl:Class ;
          rdfs:subClassOf cco:Container ;
          obo:IAO_0000117 "Ruoyu Yang" ;
          cco:definition "A container that typically is square or rectangular with lid, flat base and sides," ;
          rdfs:label "Box" .


###  http://www.semanticweb.org/ontologies/ToolOntology#BreatheProtectionTool
tool:BreatheProtectionTool rdf:type owl:Class ;
                            rdfs:subClassOf tool:PersonalProtectionTool ;
                            obo:IAO_0000115 "A personal protection artifact that is designed to protect the recipitory systems of persons from gas or debris."@en ;
                            obo:IAO_0000117 "Ruoyu Yang" ;
                            rdfs:label "Respiratory System Protection Tool" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Broom
tool:Broom rdf:type owl:Class ;
            rdfs:subClassOf tool:ManualCleaningTool ;
            obo:IAO_0000117 "Ruoyu Yang" ;
            cco:definition "A manual cleaning artifact that is designed for sweeping debris from a floor with a long-handled brush of bristles or twigs while the user of the broom is in an upright position."@en ;
            rdfs:label "Broom" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Brush
tool:Brush rdf:type owl:Class ;
            rdfs:subClassOf tool:ManualCleaningTool ;
            obo:IAO_0000115 "A manual cleaning artifact that is designed for cleaning, grooming hair, make up, painting, surface finishing, usually consisting of bristles, wire or other filaments."@en ;
            obo:IAO_0000117 "Ruoyu Yang" ;
            obo:IAO_0000119 "https://en.wikipedia.org/wiki/Brush" ;
            rdfs:label "Brush" .


###  http://www.semanticweb.org/ontologies/ToolOntology#BrushPen
tool:BrushPen rdf:type owl:Class ;
               rdfs:subClassOf tool:WritingTool ;
               obo:IAO_0000117 "Ruoyu Yang" ;
               cco:definition "A writing artifact that is made by animals' hair, mainly used n calligraphy and drawing."@en ;
               rdfs:label "Brush Pen" .


###  http://www.semanticweb.org/ontologies/ToolOntology#BuffingWheel
tool:BuffingWheel rdf:type owl:Class ;
                   rdfs:subClassOf tool:PolishingTool ;
                   obo:IAO_0000115 "A polishing artifact that is designed for shining and polishing, covered with a soft material, such as lamb's wool or leather."@en ;
                   obo:IAO_0000117 "Ruoyu Yang" ;
                   obo:IAO_0000119 "http://www.thefreedictionary.com/buffing+wheel" ;
                   rdfs:label "Buffing Wheel" .


###  http://www.semanticweb.org/ontologies/ToolOntology#BurrGrinder
tool:BurrGrinder rdf:type owl:Class ;
                  rdfs:subClassOf tool:AbradingTool ;
                  obo:IAO_0000115 "An abrading artifact that is designed for grinding hard, small food products between two revolving abrasive surfaces separated by a distance usually set by the user."@en ;
                  obo:IAO_0000117 "Ruoyu Yang" ;
                  obo:IAO_0000119 "https://en.wikipedia.org/wiki/Burr_mill" ;
                  cco:elucidation "When the two surfaces of grinder are set far apart, the resulting ground material is coarser. When the two surfaces are set closer together, the resulting ground material is finer and smaller." ;
                  rdfs:label "Burr Grinder" .


###  http://www.semanticweb.org/ontologies/ToolOntology#CableTie
tool:CableTie rdf:type owl:Class ;
               rdfs:subClassOf tool:JoinTool ;
               obo:IAO_0000115 "A join artifact that is designed for holding objects together, and which is often made of cables or wires."@en ;
               obo:IAO_0000117 "Ruoyu Yang" ;
               rdfs:label "Cable Tie" .


###  http://www.semanticweb.org/ontologies/ToolOntology#CalculatingTool
tool:CalculatingTool rdf:type owl:Class ;
                      rdfs:subClassOf cco:Tool ;
                      obo:IAO_0000117 "Ruoyu Yang" ;
                      obo:IAO_0000119 "https://en.wikipedia.org/wiki/Calculation" ;
                      cco:definition "An artifact that is designed for transforming mathematical information content entities into one or more mathematical information content entities."@en ;
                      rdfs:label "Calculating Artifact" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Calculator
tool:Calculator rdf:type owl:Class ;
                 rdfs:subClassOf tool:CalculatingTool ;
                 obo:IAO_0000117 "Ruoyu Yang" ;
                 obo:IAO_0000119 "https://en.wikipedia.org/wiki/Calculator" ;
                 cco:definition "A calculating artifact that is designed for performing calculations, ranging from basic arithmetic to complex mathematics."@en ;
                 rdfs:label "Calculator" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Caliper
tool:Caliper rdf:type owl:Class ;
              rdfs:subClassOf tool:DimensionalMeasurementTool ;
              obo:IAO_0000117 "Ruoyu Yang" ;
              obo:IAO_0000119 "https://en.wikipedia.org/wiki/Calipers" ;
              cco:definition "A spatial measurement artifact that is designed for measuring the distance between two opposite sides of an object."@en ;
              rdfs:label "Caliper" .


###  http://www.semanticweb.org/ontologies/ToolOntology#CapacitanceTestFixture
tool:CapacitanceTestFixture rdf:type owl:Class ;
                             rdfs:subClassOf tool:Fixture ;
                             cco:definition "A fixture that is designed for holding a device during testing in place, allowing the device to be tested in a capacitance test."@en ;
                             cco:term_editor "J. Neil Otte and Binbin Zhang"@en ;
                             rdfs:label "CapacitanceTest Fixture" .


###  http://www.semanticweb.org/ontologies/ToolOntology#CarpetCleaner
tool:CarpetCleaner rdf:type owl:Class ;
                    rdfs:subClassOf tool:ElectricCleaningTool ;
                    obo:IAO_0000117 "Ruoyu Yang" ;
                    obo:IAO_0000119 "https://en.wikipedia.org/wiki/Carpet_cleaning" ;
                    cco:definition "An electric cleaning artifact that is designed for removing stains, dirt, and allergens from carpet."@en ;
                    rdfs:label "Carpet Cleaner" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Cart
tool:Cart rdf:type owl:Class ;
           rdfs:subClassOf tool:TransportationTool ;
           obo:IAO_0000117 "Ruoyu Yang" ;
           obo:IAO_0000119 "https://en.wikipedia.org/wiki/Cart" ;
           cco:definition "A transportation artifact that is designed for carrying loads and pulled by a horse with two or four wheels."@en ;
           rdfs:label "Cart" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Caster
tool:Caster rdf:type owl:Class ;
             rdfs:subClassOf cco:MachineElement ;
             obo:IAO_0000117 "Ruoyu Yang" ;
             cco:definition "A machine element that is mounted to a larger object that enables relatively easy rolling movement of the object." ;
             rdfs:label "Caster" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Chiller
tool:Chiller rdf:type owl:Class ;
              rdfs:subClassOf cco:MachineElement ;
              obo:IAO_0000115 "A machine element that is designed for removing heat from a liquid via a vapor-compression or absorption refrigeration cycle." ;
              obo:IAO_0000117 "Ruoyu Yang" ;
              obo:IAO_0000119 "https://en.wikipedia.org/wiki/Chiller" ;
              rdfs:label "Chiller" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Chisel
tool:Chisel rdf:type owl:Class ;
             rdfs:subClassOf tool:CuttingTool ;
             obo:IAO_0000115 "A cutting artifact that is designed for carving or cutting a hard material such as wood, stone, or metal by hand, struck with a mallet, or mechanical power."@en ;
             obo:IAO_0000117 "Ruoyu Yang" ;
             obo:IAO_0000119 "https://en.wikipedia.org/wiki/Chisel" ;
             rdfs:label "Chisel" .


###  http://www.semanticweb.org/ontologies/ToolOntology#CircuitBoardFunctionalTester
tool:CircuitBoardFunctionalTester rdf:type owl:Class ;
                                   rdfs:subClassOf tool:TestingTool ;
                                   cco:definition "A testing artifact that is designed for testing a circuit board function."@en ;
                                   cco:term_editor "J. Neil Otte"@en ;
                                   rdfs:label "CircuitBoard Functional Tester" .


###  http://www.semanticweb.org/ontologies/ToolOntology#CircuitBoardTester
tool:CircuitBoardTester rdf:type owl:Class ;
                         rdfs:subClassOf tool:TestingTool ;
                         cco:definition "A testing artifact that is designed for testing a circuit board."@en ;
                         cco:term_editor "J. Neil Otte"@en ;
                         rdfs:label "CircuitBoard Tester" .


###  http://www.semanticweb.org/ontologies/ToolOntology#CircularSaw
tool:CircularSaw rdf:type owl:Class ;
                  rdfs:subClassOf tool:SawingTool ;
                  obo:IAO_0000115 "A sawing artifact that is designed for cutting different materials using a rotary motion spinning around an arbor by using a toothed or abrasive disc or blade."@en ;
                  obo:IAO_0000117 "Ruoyu Yang" ;
                  obo:IAO_0000119 "https://en.wikipedia.org/wiki/Circular_saw" ;
                  rdfs:label "Circular Saw" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Clamp
tool:Clamp rdf:type owl:Class ;
            rdfs:subClassOf tool:JoinTool ;
            obo:IAO_0000115 "A join artifact that is designed for holding or securing objects tightly together to prevent movement or separation through the application of inward pressure."@en ;
            obo:IAO_0000117 "Ruoyu Yang" ;
            obo:IAO_0000119 "https://en.wikipedia.org/wiki/Clamp_(artifact)" ;
            rdfs:label "Clamp" .


###  http://www.semanticweb.org/ontologies/ToolOntology#CleaningTool
tool:CleaningTool rdf:type owl:Class ;
                   rdfs:subClassOf cco:Tool ;
                   obo:IAO_0000115 "An artifact that is designed for removing debris, especially by washing, filtering, wiping, or brushing."@en ;
                   obo:IAO_0000117 "Ruoyu Yang" ;
                   rdfs:label "Cleaning Artifact" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Compass
tool:Compass rdf:type owl:Class ;
              rdfs:subClassOf tool:LocationMeasurementTool ;
              obo:IAO_0000117 "Ruoyu Yang" ;
              obo:IAO_0000119 "https://en.wikipedia.org/wiki/Compass" ;
              cco:definition "A location measurement artifact that is designed for navigation and orientation that shows direction relative to the geographic cardinal directions."@en ;
              rdfs:label "Compass" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Compressor
tool:Compressor rdf:type owl:Class ;
                 rdfs:subClassOf cco:MachineElement ;
                 obo:IAO_0000115 "A machine element that is designed for increasing the pressure of fluid, transfering high pressure gas and charging gas cylinders and scaveng." ;
                 obo:IAO_0000117 "Ruoyu Yang" ;
                 rdfs:label "Compressor" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Conveyor
tool:Conveyor rdf:type owl:Class ;
               rdfs:subClassOf tool:TransportationTool ;
               obo:IAO_0000117 "Ruoyu Yang" ;
               obo:IAO_0000119 "https://en.wikipedia.org/wiki/Conveyor_system" ;
               cco:definition "A transportation artifact that moves materials from one location to another, usually useful in applications involving the transportation of heavy or bulky materials."@en ;
               cco:elucidation "Conveyor systems allow quick and efficient transportation for a wide variety of materials, which make them very popular in the material handling and packaging industries." ;
               rdfs:label "Conveyor" .


###  http://www.semanticweb.org/ontologies/ToolOntology#CounterBalance
tool:CounterBalance rdf:type owl:Class ;
                     rdfs:subClassOf tool:WeightMeasurementTool ;
                     obo:IAO_0000117 "Ruoyu Yang" ;
                     cco:definition "A weight measurement artifact that is designed for measuring weight or calculating mass by balancing the force due to gravity against the force on a spring."@en ;
                     rdfs:label "Counter Balance" .


###  http://www.semanticweb.org/ontologies/ToolOntology#Crane
tool:Crane rdf:type owl:Class ;
            rdfs:subClassOf tool:LiftingTool ;
            obo:IAO_0000115 "A lifting artifact that is designed for lifting and lowering materials and moving them horizontally, usually equipped with a hoist rope, wire ropes or chains, and sheaves."@en ;
            obo:IAO_0000117 "Ruoyu Yang" ;
            obo:IAO_0000119 "https://en.wikipedia.org/wiki/Crane_(machine)" ;
            rdfs:label "Crane" .


